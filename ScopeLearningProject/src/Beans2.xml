<?xml version = "1.0" encoding = "UTF-8"?>




<beans xmlns = "http://www.springframework.org/schema/beans" 
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	
	<bean id = "Singleton" class = "com.tutorialspoint.HelloWorld2" scope = "singleton">
		<!-- 
		this property has been eliminated because the object itself in the mainapp is
		defining the message, not the bean
		
		<property name = "message" value = "Hello World" /> -->
	</bean>
	

	<bean id = "Prototype" class = "com.tutorialspoint.HelloWorld2" scope = "prototype">
		
	</bean>




</beans>